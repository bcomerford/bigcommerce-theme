{"components/products/single-review":"<div class=\"layout-grid-item\" itemscope itemprop=\"review\" itemtype=\"http://schema.org/Review\">\n  <div class='sr-only' itemprop=\"itemReviewed\" itemscope itemtype=\"http://schema.org/Product\" aria-hidden=\"true\">\n    <span itemprop=\"name\">{{product_title}}</span>\n  </div>\n  <span class=\"rating-stars-readonly\" aria-hidden=\"true\" itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\n    {{> components/common/rating rating=rating}}\n    <meta class=\"sr-only\" itemprop=\"ratingValue\" content=\"{{rating}}\">\n  </span>\n\n  <h3 class=\"single-review-title\" itemprop=\"name\">{{title}}</h3>\n\n  <span class=\"single-review-author\" itemprop=\"author\" itemscope itemtype=\"http://schema.org/Person\">\n    {{#if name}}\n      {{lang 'product.reviews.posted_on_by' date=date name=name}}\n    {{else}}\n      {{lang 'product.reviews.posted_on' date=date}}\n    {{/if}}\n    <meta itemprop=\"name\" content=\"{{name}}\">\n  </span>\n\n  <p class=\"single-review-text\" itemprop=\"reviewBody\">{{{this.text}}}</p>\n\n</div>\n","components/common/rating":"\n{{#for 1 5}}\n    <i class=\"rating-star {{#if ../rating '>=' $index}}icon-star{{else}}icon-star-empty{{/if}}\"></i>\n{{/for}}\n"}